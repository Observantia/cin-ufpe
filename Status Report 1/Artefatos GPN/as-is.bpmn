<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" 
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" 
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" 
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI" 
                  id="Definitions_1" 
                  targetNamespace="http://bpmn.io/schema/bpmn">

  <bpmn:collaboration id="Collaboration_1">
    <bpmn:participant id="Participant_SetorRequisitante" name="Setor Requisitante" processRef="Process_SetorRequisitante" />
    <bpmn:participant id="Participant_CoordCompras" name="Coordenação de Compras" processRef="Process_CoordCompras" />
    <bpmn:participant id="Participant_Procuradoria" name="Procuradoria Jurídica" processRef="Process_Procuradoria" />
    <bpmn:participant id="Participant_DLC" name="DLC (Reitoria)" processRef="Process_DLC" />
    <bpmn:participant id="Participant_Autoridade" name="Autoridade Competente" processRef="Process_Autoridade" />
    
    <bpmn:messageFlow id="Flow_DFD" sourceRef="Task_ElaborarDFD" targetRef="Event_RecebeDFD" />
    <bpmn:messageFlow id="Flow_ProcessoSEI" sourceRef="Task_AbrirProcesso" targetRef="Event_RecebeProcesso" />
    <bpmn:messageFlow id="Flow_ParaProcuradoria" sourceRef="Task_EnviarProcuradoria" targetRef="Event_RecebeProcuradoria" />
    <bpmn:messageFlow id="Flow_Parecer" sourceRef="Task_AnalisarJuridico" targetRef="Event_RecebeParecer" />
    <bpmn:messageFlow id="Flow_ParaDLC" sourceRef="Gateway_ValorDLC" targetRef="Event_RecebeDLC" />
    <bpmn:messageFlow id="Flow_ConformidadeDLC" sourceRef="Task_AnalisarConformidade" targetRef="Event_RecebeConformidade" />
    <bpmn:messageFlow id="Flow_ParaAutoridade" sourceRef="Task_EnviarAutorizacao" targetRef="Event_RecebeAutorizacao" />
    <bpmn:messageFlow id="Flow_Autorizacao" sourceRef="Task_AutorizarLicitacao" targetRef="Event_RecebeAutorizacao2" />
    <bpmn:messageFlow id="Flow_VoltaRequisitante" sourceRef="Gateway_AjusteFaseInterna" targetRef="Event_RecebeAjustes" />
  </bpmn:collaboration>

  <!-- Processo do Setor Requisitante -->
  <bpmn:process id="Process_SetorRequisitante" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Necessidade Identificada">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:task id="Task_ElaborarDFD" name="Elaborar DFD">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_IncluirPCA" name="Incluir no PCA (Sistema Compras)">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_AbrirProcesso" name="Abrir Processo no SEI">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_DefinirEquipe" name="Definir Equipe de Planejamento">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:subProcess id="SubProcess_FaseInterna" name="Elaborar Documentos Fase Interna">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:incoming>Flow_AjusteRequisitante</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
      
      <bpmn:startEvent id="Start_FaseInterna">
        <bpmn:outgoing>Flow_Sub1</bpmn:outgoing>
      </bpmn:startEvent>
      
      <bpmn:parallelGateway id="Gateway_InicioDocumentos">
        <bpmn:incoming>Flow_Sub1</bpmn:incoming>
        <bpmn:outgoing>Flow_Sub2</bpmn:outgoing>
        <bpmn:outgoing>Flow_Sub3</bpmn:outgoing>
        <bpmn:outgoing>Flow_Sub4</bpmn:outgoing>
        <bpmn:outgoing>Flow_Sub5</bpmn:outgoing>
        <bpmn:outgoing>Flow_Sub6</bpmn:outgoing>
      </bpmn:parallelGateway>
      
      <bpmn:task id="Task_ETP" name="Elaborar ETP">
        <bpmn:incoming>Flow_Sub2</bpmn:incoming>
        <bpmn:outgoing>Flow_Sub7</bpmn:outgoing>
      </bpmn:task>
      
      <bpmn:task id="Task_MapaRisco" name="Elaborar Mapa de Risco">
        <bpmn:incoming>Flow_Sub3</bpmn:incoming>
        <bpmn:outgoing>Flow_Sub8</bpmn:outgoing>
      </bpmn:task>
      
      <bpmn:task id="Task_TR" name="Elaborar Termo de Referência">
        <bpmn:incoming>Flow_Sub4</bpmn:incoming>
        <bpmn:outgoing>Flow_Sub9</bpmn:outgoing>
      </bpmn:task>
      
      <bpmn:task id="Task_MemoriaCalculo" name="Elaborar Memória de Cálculo">
        <bpmn:incoming>Flow_Sub5</bpmn:incoming>
        <bpmn:outgoing>Flow_Sub10</bpmn:outgoing>
      </bpmn:task>
      
      <bpmn:task id="Task_PesquisaPrecos" name="Realizar Pesquisa de Preços">
        <bpmn:incoming>Flow_Sub6</bpmn:incoming>
        <bpmn:outgoing>Flow_Sub11</bpmn:outgoing>
      </bpmn:task>
      
      <bpmn:parallelGateway id="Gateway_FimDocumentos">
        <bpmn:incoming>Flow_Sub7</bpmn:incoming>
        <bpmn:incoming>Flow_Sub8</bpmn:incoming>
        <bpmn:incoming>Flow_Sub9</bpmn:incoming>
        <bpmn:incoming>Flow_Sub10</bpmn:incoming>
        <bpmn:incoming>Flow_Sub11</bpmn:incoming>
        <bpmn:outgoing>Flow_Sub12</bpmn:outgoing>
      </bpmn:parallelGateway>
      
      <bpmn:endEvent id="End_FaseInterna">
        <bpmn:incoming>Flow_Sub12</bpmn:incoming>
      </bpmn:endEvent>
      
      <bpmn:sequenceFlow id="Flow_Sub1" sourceRef="Start_FaseInterna" targetRef="Gateway_InicioDocumentos" />
      <bpmn:sequenceFlow id="Flow_Sub2" sourceRef="Gateway_InicioDocumentos" targetRef="Task_ETP" />
      <bpmn:sequenceFlow id="Flow_Sub3" sourceRef="Gateway_InicioDocumentos" targetRef="Task_MapaRisco" />
      <bpmn:sequenceFlow id="Flow_Sub4" sourceRef="Gateway_InicioDocumentos" targetRef="Task_TR" />
      <bpmn:sequenceFlow id="Flow_Sub5" sourceRef="Gateway_InicioDocumentos" targetRef="Task_MemoriaCalculo" />
      <bpmn:sequenceFlow id="Flow_Sub6" sourceRef="Gateway_InicioDocumentos" targetRef="Task_PesquisaPrecos" />
      <bpmn:sequenceFlow id="Flow_Sub7" sourceRef="Task_ETP" targetRef="Gateway_FimDocumentos" />
      <bpmn:sequenceFlow id="Flow_Sub8" sourceRef="Task_MapaRisco" targetRef="Gateway_FimDocumentos" />
      <bpmn:sequenceFlow id="Flow_Sub9" sourceRef="Task_TR" targetRef="Gateway_FimDocumentos" />
      <bpmn:sequenceFlow id="Flow_Sub10" sourceRef="Task_MemoriaCalculo" targetRef="Gateway_FimDocumentos" />
      <bpmn:sequenceFlow id="Flow_Sub11" sourceRef="Task_PesquisaPrecos" targetRef="Gateway_FimDocumentos" />
      <bpmn:sequenceFlow id="Flow_Sub12" sourceRef="Gateway_FimDocumentos" targetRef="End_FaseInterna" />
    </bpmn:subProcess>
    
    <bpmn:intermediateCatchEvent id="Event_RecebeAjustes" name="Recebe Solicitação de Ajustes">
      <bpmn:outgoing>Flow_AjusteRequisitante</bpmn:outgoing>
      <bpmn:messageEventDefinition />
    </bpmn:intermediateCatchEvent>
    
    <bpmn:endEvent id="End_Requisitante">
      <bpmn:incoming>Flow_6</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_ElaborarDFD" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_ElaborarDFD" targetRef="Task_IncluirPCA" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_IncluirPCA" targetRef="Task_AbrirProcesso" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Task_AbrirProcesso" targetRef="Task_DefinirEquipe" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Task_DefinirEquipe" targetRef="SubProcess_FaseInterna" />
    <bpmn:sequenceFlow id="Flow_6" sourceRef="SubProcess_FaseInterna" targetRef="End_Requisitante" />
    <bpmn:sequenceFlow id="Flow_AjusteRequisitante" sourceRef="Event_RecebeAjustes" targetRef="SubProcess_FaseInterna" />
  </bpmn:process>

  <!-- Processo da Coordenação de Compras -->
  <bpmn:process id="Process_CoordCompras" isExecutable="true">
    <bpmn:startEvent id="Event_RecebeDFD" name="Recebe DFD">
      <bpmn:outgoing>Flow_C1</bpmn:outgoing>
      <bpmn:messageEventDefinition />
    </bpmn:startEvent>
    
    <bpmn:task id="Task_ConsolidarPCA" name="Consolidar e Aprovar PCA">
      <bpmn:incoming>Flow_C1</bpmn:incoming>
      <bpmn:outgoing>Flow_C2</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:intermediateCatchEvent id="Event_RecebeProcesso" name="Recebe Processo SEI">
      <bpmn:incoming>Flow_C2</bpmn:incoming>
      <bpmn:outgoing>Flow_C3</bpmn:outgoing>
      <bpmn:messageEventDefinition />
    </bpmn:intermediateCatchEvent>
    
    <bpmn:task id="Task_ChecagemInicial" name="Realizar Checagem Inicial">
      <bpmn:incoming>Flow_C3</bpmn:incoming>
      <bpmn:outgoing>Flow_C4</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:exclusiveGateway id="Gateway_TipoContratacao" name="Tipo de Contratação?">
      <bpmn:incoming>Flow_C4</bpmn:incoming>
      <bpmn:outgoing>Flow_C5</bpmn:outgoing>
      <bpmn:outgoing>Flow_C6</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:task id="Task_PublicarIRP" name="Publicar IRP">
      <bpmn:incoming>Flow_C5</bpmn:incoming>
      <bpmn:outgoing>Flow_C7</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_ConsolidarParticipantes" name="Consolidar Participantes IRP">
      <bpmn:incoming>Flow_C7</bpmn:incoming>
      <bpmn:outgoing>Flow_C8</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_RevisarTR" name="Revisar TR">
      <bpmn:incoming>Flow_C8</bpmn:incoming>
      <bpmn:outgoing>Flow_C9</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_ElaborarInstrumentos" name="Elaborar Edital e Minutas">
      <bpmn:incoming>Flow_C9</bpmn:incoming>
      <bpmn:incoming>Flow_C6</bpmn:incoming>
      <bpmn:outgoing>Flow_C10</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:exclusiveGateway id="Gateway_NecessitaProcuradoria" name="Necessita Análise Jurídica?">
      <bpmn:incoming>Flow_C10</bpmn:incoming>
      <bpmn:outgoing>Flow_C11</bpmn:outgoing>
      <bpmn:outgoing>Flow_C12</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:task id="Task_EnviarProcuradoria" name="Enviar para Procuradoria">
      <bpmn:incoming>Flow_C11</bpmn:incoming>
      <bpmn:outgoing>Flow_C13</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:intermediateCatchEvent id="Event_RecebeParecer" name="Recebe Parecer">
      <bpmn:incoming>Flow_C13</bpmn:incoming>
      <bpmn:outgoing>Flow_C14</bpmn:outgoing>
      <bpmn:messageEventDefinition />
    </bpmn:intermediateCatchEvent>
    
    <bpmn:task id="Task_AtenderRecomendacoes" name="Atender Recomendações">
      <bpmn:incoming>Flow_C14</bpmn:incoming>
      <bpmn:outgoing>Flow_C15</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:exclusiveGateway id="Gateway_AjusteFaseInterna" name="Ajuste na Fase Interna?">
      <bpmn:incoming>Flow_C15</bpmn:incoming>
      <bpmn:outgoing>Flow_C16</bpmn:outgoing>
      <bpmn:outgoing>Flow_C17</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:exclusiveGateway id="Gateway_ValorDLC" name="Valor > R$ 1 milhão?">
      <bpmn:incoming>Flow_C16</bpmn:incoming>
      <bpmn:incoming>Flow_C12</bpmn:incoming>
      <bpmn:outgoing>Flow_C18</bpmn:outgoing>
      <bpmn:outgoing>Flow_C19</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:intermediateCatchEvent id="Event_RecebeConformidade" name="Recebe Conformidade DLC">
      <bpmn:incoming>Flow_C18</bpmn:incoming>
      <bpmn:outgoing>Flow_C20</bpmn:outgoing>
      <bpmn:messageEventDefinition />
    </bpmn:intermediateCatchEvent>
    
    <bpmn:task id="Task_EnviarAutorizacao" name="Enviar para Autorização">
      <bpmn:incoming>Flow_C20</bpmn:incoming>
      <bpmn:incoming>Flow_C19</bpmn:incoming>
      <bpmn:outgoing>Flow_C21</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:intermediateCatchEvent id="Event_RecebeAutorizacao2" name="Recebe Autorização">
      <bpmn:incoming>Flow_C21</bpmn:incoming>
      <bpmn:outgoing>Flow_C22</bpmn:outgoing>
      <bpmn:messageEventDefinition />
    </bpmn:intermediateCatchEvent>
    
    <bpmn:task id="Task_PublicarEdital" name="Publicar Edital">
      <bpmn:incoming>Flow_C22</bpmn:incoming>
      <bpmn:outgoing>Flow_C23</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:endEvent id="End_Compras" name="Fase de Seleção">
      <bpmn:incoming>Flow_C23</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:sequenceFlow id="Flow_C1" sourceRef="Event_RecebeDFD" targetRef="Task_ConsolidarPCA" />
    <bpmn:sequenceFlow id="Flow_C2" sourceRef="Task_ConsolidarPCA" targetRef="Event_RecebeProcesso" />
    <bpmn:sequenceFlow id="Flow_C3" sourceRef="Event_RecebeProcesso" targetRef="Task_ChecagemInicial" />
    <bpmn:sequenceFlow id="Flow_C4" sourceRef="Task_ChecagemInicial" targetRef="Gateway_TipoContratacao" />
    <bpmn:sequenceFlow id="Flow_C5" sourceRef="Gateway_TipoContratacao" targetRef="Task_PublicarIRP" name="Licitação" />
    <bpmn:sequenceFlow id="Flow_C6" sourceRef="Gateway_TipoContratacao" targetRef="Task_ElaborarInstrumentos" name="Dispensa" />
    <bpmn:sequenceFlow id="Flow_C7" sourceRef="Task_PublicarIRP" targetRef="Task_ConsolidarParticipantes" />
    <bpmn:sequenceFlow id="Flow_C8" sourceRef="Task_ConsolidarParticipantes" targetRef="Task_RevisarTR" />
    <bpmn:sequenceFlow id="Flow_C9" sourceRef="Task_RevisarTR" targetRef="Task_ElaborarInstrumentos" />
    <bpmn:sequenceFlow id="Flow_C10" sourceRef="Task_ElaborarInstrumentos" targetRef="Gateway_NecessitaProcuradoria" />
    <bpmn:sequenceFlow id="Flow_C11" sourceRef="Gateway_NecessitaProcuradoria" targetRef="Task_EnviarProcuradoria" name="Sim" />
    <bpmn:sequenceFlow id="Flow_C12" sourceRef="Gateway_NecessitaProcuradoria" targetRef="Gateway_ValorDLC" name="Não" />
    <bpmn:sequenceFlow id="Flow_C13" sourceRef="Task_EnviarProcuradoria" targetRef="Event_RecebeParecer" />
    <bpmn:sequenceFlow id="Flow_C14" sourceRef="Event_RecebeParecer" targetRef="Task_AtenderRecomendacoes" />
    <bpmn:sequenceFlow id="Flow_C15" sourceRef="Task_AtenderRecomendacoes" targetRef="Gateway_AjusteFaseInterna" />
    <bpmn:sequenceFlow id="Flow_C16" sourceRef="Gateway_AjusteFaseInterna" targetRef="Gateway_ValorDLC" name="Não" />
    <bpmn:sequenceFlow id="Flow_C17" sourceRef="Gateway_AjusteFaseInterna" targetRef="Gateway_AjusteFaseInterna" name="Sim (Volta ao Requisitante)" />
    <bpmn:sequenceFlow id="Flow_C18" sourceRef="Gateway_ValorDLC" targetRef="Event_RecebeConformidade" name="Sim" />
    <bpmn:sequenceFlow id="Flow_C19" sourceRef="Gateway_ValorDLC" targetRef="Task_EnviarAutorizacao" name="Não" />
    <bpmn:sequenceFlow id="Flow_C20" sourceRef="Event_RecebeConformidade" targetRef="Task_EnviarAutorizacao" />
    <bpmn:sequenceFlow id="Flow_C21" sourceRef="Task_EnviarAutorizacao" targetRef="Event_RecebeAutorizacao2" />
    <bpmn:sequenceFlow id="Flow_C22" sourceRef="Event_RecebeAutorizacao2" targetRef="Task_PublicarEdital" />
    <bpmn:sequenceFlow id="Flow_C23" sourceRef="Task_PublicarEdital" targetRef="End_Compras" />
  </bpmn:process>

  <!-- Processo da Procuradoria Jurídica -->
  <bpmn:process id="Process_Procuradoria" isExecutable="true">
    <bpmn:startEvent id="Event_RecebeProcuradoria" name="Recebe Processo">
      <bpmn:messageEventDefinition />
      <bpmn:outgoing>Flow_P1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:task id="Task_AnalisarJuridico" name="Analisar e Emitir Parecer">
      <bpmn:incoming>Flow_P1</bpmn:incoming>
      <bpmn:outgoing>Flow_P2</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:endEvent id="End_Procuradoria">
      <bpmn:incoming>Flow_P2</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:sequenceFlow id="Flow_P1" sourceRef="Event_RecebeProcuradoria" targetRef="Task_AnalisarJuridico" />
    <bpmn:sequenceFlow id="Flow_P2" sourceRef="Task_AnalisarJuridico" targetRef="End_Procuradoria" />
  </bpmn:process>

  <!-- Processo da DLC -->
  <bpmn:process id="Process_DLC" isExecutable="true">
    <bpmn:startEvent id="Event_RecebeDLC" name="Recebe Processo (> R$ 1 milhão)">
      <bpmn:messageEventDefinition />
      <bpmn:outgoing>Flow_D1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:task id="Task_AnalisarConformidade" name="Analisar Conformidade">
      <bpmn:incoming>Flow_D1</bpmn:incoming>
      <bpmn:outgoing>Flow_D2</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:endEvent id="End_DLC">
      <bpmn:incoming>Flow_D2</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:sequenceFlow id="Flow_D1" sourceRef="Event_RecebeDLC" targetRef="Task_AnalisarConformidade" />
    <bpmn:sequenceFlow id="Flow_D2" sourceRef="Task_AnalisarConformidade" targetRef="End_DLC" />
  </bpmn:process>

  <!-- Processo da Autoridade Competente -->
  <bpmn:process id="Process_Autoridade" isExecutable="true">
    <bpmn:startEvent id="Event_RecebeAutorizacao" name="Recebe para Autorização">
      <bpmn:messageEventDefinition />
      <bpmn:outgoing>Flow_A1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:exclusiveGateway id="Gateway_Autoridade" name="Qual Autoridade?">
      <bpmn:incoming>Flow_A1</bpmn:incoming>
      <bpmn:outgoing>Flow_A2</bpmn:outgoing>
      <bpmn:outgoing>Flow_A3</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:task id="Task_AutorizarDiretor" name="Diretor-Geral Autoriza (até R$ 1 mi)">
      <bpmn:incoming>Flow_A2</bpmn:incoming>
      <bpmn:outgoing>Flow_A4</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_AutorizarReitor" name="Reitor Autoriza (> R$ 1 mi)">
      <bpmn:incoming>Flow_A3</bpmn:incoming>
      <bpmn:outgoing>Flow_A5</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:task id="Task_AutorizarLicitacao" name="Autorizar Licitação">
      <bpmn:incoming>Flow_A4</bpmn:incoming>
      <bpmn:incoming>Flow_A5</bpmn:incoming>
      <bpmn:outgoing>Flow_A6</bpmn:outgoing>
    </bpmn:task>
    
    <bpmn:endEvent id="End_Autoridade">
      <bpmn:incoming>Flow_A6</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:sequenceFlow id="Flow_A1" sourceRef="Event_RecebeAutorizacao" targetRef="Gateway_Autoridade" />
    <bpmn:sequenceFlow id="Flow_A2" sourceRef="Gateway_Autoridade" targetRef="Task_AutorizarDiretor" name="Até R$ 1 mi" />
    <bpmn:sequenceFlow id="Flow_A3" sourceRef="Gateway_Autoridade" targetRef="Task_AutorizarReitor" name="> R$ 1 mi" />
    <bpmn:sequenceFlow id="Flow_A4" sourceRef="Task_AutorizarDiretor" targetRef="Task_AutorizarLicitacao" />
    <bpmn:sequenceFlow id="Flow_A5" sourceRef="Task_AutorizarReitor" targetRef="Task_AutorizarLicitacao" />
    <bpmn:sequenceFlow id="Flow_A6" sourceRef="Task_AutorizarLicitacao" targetRef="End_Autoridade" />
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Collaboration_1">
      <!-- Layout dos pools e elementos seria definido aqui com coordenadas específicas -->
      <!-- Para simplicidade, estou incluindo apenas a estrutura básica dos elementos visuais -->
      
      <bpmndi:BPMNShape id="Participant_SetorRequisitante_di" bpmnElement="Participant_SetorRequisitante" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="1400" height="250" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Participant_CoordCompras_di" bpmnElement="Participant_CoordCompras" isHorizontal="true">
        <dc:Bounds x="160" y="350" width="1400" height="300" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Participant_Procuradoria_di" bpmnElement="Participant_Procuradoria" isHorizontal="true">
        <dc:Bounds x="160" y="670" width="1400" height="150" />
